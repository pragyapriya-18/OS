(STRING OPERATION):-->

#!/bin/sh

a="abc"
b="egf"

if [ $a = $b ]
then
echo "$a = $b: a is equal to b"
else
echo "$a = $b: a is not equal to b"
fi

if [ $a! = $b ]
then
echo " $a! = $b " a is not equal to b"
else
echo "a is equal to b"
fi 

if [ -Z$a ]
then 
echo "-Z$a: string length zero"
else
echo "-Z$a: string length is not zero"
fi

if [ -n$a ]
then 
echo "-n$a: string lenght not zero"
else
echo "-n$a: string length is zero"
fi

if [ $a ]
then 
echo "$a: string is not empty"
else
echo "$a: string is empty"
fi

Mkdir pragya(folder name)
Cd priya(file name)
touch priya. sh
chmod +x priya. sh
. /priya.sh

