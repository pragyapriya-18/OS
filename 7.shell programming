(FILE OPERATION):-->

#!/bin/sh
file = "str.sh

if[ -r $file ]
then
echo "file has read access"
else
echo "file didn't have read access"
fi

if [ -w $file ]
then 
echo "file has write access"
else
echo "don't have write permission"
fi

if [ -x $file ]
then 
echo "file have execute permission"
else
echo "file has no execute permission"
fi

if [ -f $file ]
then 
echo "file is an ordinary file"
else
echo "file is special"
fi

if [ -d $file ]
then 
echo "file is directory"
else
echo "not directory"
fi

if [ -s $file ]
then 
echo "file size not zero"
else
echo "file size zero"
fi

if [ -e $file ]
then 
echo "file exists"
else
echo "file does not exists"
fi

chmod +x file.sh
./file.sh